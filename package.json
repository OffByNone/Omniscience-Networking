{
	"name": "omniscience-networking",
	"version": "0.8.1",
	"description": "Networking For Omniscience",
	"title": "Omniscience Networking",
	"main": "./bin/index.js",
	"author": "OffByNone",
	"license": "GNU GPL v3",
	"homepage": "https://github.com/offbynone/omniscience-networking",
	"bugs": {
		"url": "https://github.com/offbynone/omniscience-networking/issues"
	},
	"keywords": [
		"omniscience"
	],
	"engines": {
		"Firefox": ">=38.0a1",
		"Fennec": ">=38.0a1"
	},
	"repository": {
		"type": "git",
		"url": "git://github.com/offbynone/omniscience-networking.git"
	},
	"devDependencies": {
		"babel": "^5.8.21",
		"eslint": "^1.2.1",
		"jasmine": "^2.3.2",
		"jpm": "^1.0.1"
	},
	"dependencies": {
		"omniscience-sdk-resolver": "offbynone/omniscience-sdk-resolver",
		"omniscience-utilities": "offbynone/omniscience-utilities"
	},
	"scripts": {
		"lint": "./node_modules/.bin/eslint",
		"build": "./node_modules/.bin/babel ./lib --out-dir ./bin -s",
		"post": "./node_modules/.bin/jpm post --post-url=http://127.0.0.1:7999",
		"buildpost": "npm run build && npm run post",
		"compile": "npm run build && ./node_modules/.bin/jpm xpi",
		"install": "./node_modules/.bin/babel ./lib --out-dir ./bin -s",
		"test": "./node_modules/.bin/jasmine"
	}
}